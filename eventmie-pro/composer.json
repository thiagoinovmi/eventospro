{
    "name": "classiebit/eventmie-pro",
    "version": "3.0.0",
    "description": "Run your own Events business with Eventmie Pro. Use it as event ticket selling website or event management platform on your own domain.",
    "keywords": [
        "online events",
        "zoom",
        "google meet",
        "jio meet",
        "events",
        "event ticket selling website", 
        "event management platform",
        "event-management",
        "event-planning",
        "laravel",
        "laravel-package",
        "voyager",
        "vuejs"
    ],
    "license": "Commercial",
    "homepage": "https://classiebit.com/eventmie-pro",
    "authors": [
        {
            "name": "Classiebit",
            "email": "info@classiebit.com"
        }
    ],
    "require": {
        "consoletvs/charts": "^6.8",
        "barryvdh/laravel-dompdf": "^3.1",
        "laravel/socialite": "^5.18",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "tcg/voyager": "^1.8",
        "tightenco/ziggy": "^2.5",
        "usmanhalalit/laracsv": "^2.1",
        "yajra/laravel-datatables-oracle": "^11.1",
        "league/omnipay": "^3.2",
        "omnipay/paypal": "^3.0",
        "spatie/laravel-honeypot": "^4.5",
        "laravel/sanctum": "^4.0",
        "league/flysystem-aws-s3-v3": "^3.29",
        "laravel/ui": "^4.6",
        "openai-php/laravel": "^0.11.0",
        "devrabiul/laravel-cookie-consent": "^1.0"
    },
    "require-dev": {
        "laravel/framework": "~11.0" 
    },
    "autoload": {
        "psr-4": {
            "Classiebit\\Eventmie\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Classiebit\\Eventmie\\EventmieServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
